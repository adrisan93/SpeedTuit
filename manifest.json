{
  "manifest_version": 2,

  "name": "SpeedTuit",
  "description": "Integraci√≥n de Twitter en tu navegador",
  "background": {
    "scripts": ["background.js"]
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup" : "index.html"
  },
  "version":"1.0",
  "icons":{
  	"64" : "icon.png",
  	"32" : "icon.png",
  	"16" : "icon.png",
  	"128" : "icon.png"
  },
  "permissions": [
  	"contextMenus",
    "notifications",
    "https://twitter.com/i/notifications?lang=es"
  ]
}
